{
  "name": "teleport-docs-next",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "git-update": "git submodule update --remote",
    "build-loaders": "rm -rf __build && ttsc --build tsconfig.loaders.json",
    "add-symlinks": "node ./scripts/add-symlinks.js",
    "preflight": "yarn build-loaders && yarn add-symlinks",
    "dev": "yarn preflight && next dev",
    "build": "yarn preflight && next build",
    "update-and-build": "yarn git-update && yarn build",
    "start": "next start",
    "test": "jest --runInBand",
    "base:eslint": "eslint --cache --cache-location node_modules/.cache/eslint '{components,loaders,pages,scripts,test,utils}/**/*.{js,jsx,ts,tsx}'",
    "base:prettier": "prettier '{components,loaders,pages,scripts,test,utils}/**/*.{js,jsx,ts,tsx,json}'",
    "lint": "yarn base:eslint --fix && yarn base:prettier --write -l",
    "lint-check": "yarn base:eslint && yarn base:prettier --check",
    "typecheck": "tsc --noEmit --skipLibCheck --incremental --tsBuildInfoFile node_modules/.cache/tsc/tsbuildinfo --project ."
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json}": "prettier --write"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@reach/listbox": "^0.12.1",
    "@styled-system/css": "^5.1.5",
    "docsearch.js": "^2.6.3",
    "fathom-client": "^3.0.0",
    "next": "10.0.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-use": "^15.3.8",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/bundle-analyzer": "^10.0.6",
    "@svgr/webpack": "^5.5.0",
    "@types/glob": "^7.1.3",
    "@types/htmltojsx": "^0.0.29",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/rebass": "^4.0.7",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "hast-util-heading-rank": "^1.0.1",
    "hast-util-to-string": "^1.0.4",
    "hastscript": "^6.0.0",
    "htmltojsx": "^0.3.0",
    "jest": "^26.6.3",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prettier-config-standard": "^1.0.1",
    "rehype-highlight": "^4.0.0",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^8.0.0",
    "remark": "^13.0.0",
    "remark-copy-linked-files": "^1.2.0",
    "remark-frontmatter": "^2.0.0",
    "remark-rehype": "^8.0.0",
    "stringify-object": "^3.3.0",
    "ts-jest": "^26.4.4",
    "ts-transformer-imports": "^0.4.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.2",
    "unified": "^9.2.0",
    "unist-builder": "^2.0.3",
    "unist-util-visit": "^2.0.3",
    "url-loader": "^4.1.1",
    "vfile": "^4.2.1"
  }
}
